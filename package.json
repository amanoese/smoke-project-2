{
  "name": "smoke-project-2",
  "version": "0.1.0",
  "description": "My Superb Vue Project",
  "main": "client/index.js",
  "scripts": {
    "test": "testcafe all test/e2e/index.js --app \"npm run dev\" --app-init-delay 10000",
    "test-pi": "mocha test/pi/pi-test.js",
